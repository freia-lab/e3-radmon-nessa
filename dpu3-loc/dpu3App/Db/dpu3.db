record(stringin, "$(P)StringInData0") {
   field (DESC, "Data Det 0")
   field (DTYP, "stream")
   field (INP, "@dpu3.proto get_data(0) $(PORT)")
   field (PINI, "YES")
   field (SCAN, ".2 second")
#   field (SCAN, "I/O Intr")
}

