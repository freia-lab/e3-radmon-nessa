record(ai, "$(P)IntDet0-Dose") {
   field (DESC, "Internal detector 0 dose")
   field (DTYP, "stream")
   field (INP, "@dpu3.proto get_dose(0)  $(PORT)")
   field (PINI, "YES")
   field (SCAN, ".2 second")
   field (EGU, "uSv")
#   field (SCAN, "I/O Intr")
   field(PREC, "2")
}

